    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.29.0",
    "svelte": "^5.1.9",
    "svelte-check": "^4.0.4",
    "tailwindcss": "^3.4.14",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vitest": "^2.1.3"
  },
  "dependencies": {
    "@capacitor/cli": "^7.4.2",
    "@capacitor/core": "^7.4.2",
    "@capacitor/ios": "^7.4.2",
    "@floating-ui/dom": "^1.7.2",
